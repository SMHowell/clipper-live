      INS-159022_FOV_REF_VECTOR            = ( 1.0, 0.0, 0.0 )
      INS-159022_FOV_REF_ANGLE             = ( 66.0 )
      INS-159022_FOV_CROSS_ANGLE           = ( 66.0 )
      INS-159022_FOV_ANGLE_UNITS           = 'DEGREES'

